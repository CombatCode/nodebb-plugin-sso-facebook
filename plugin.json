{
	"id": "nodebb-plugin-sso-facebook",
	"name": "NodeBB Facebook SSO",
	"description": "NodeBB Plugin that allows users to login/register via their Facebook account.",
	"url": "https://github.com/julianlam/nodebb-plugin-sso-facebook",
	"library": "./library.js",
	"hooks": [
		{
			"hook": "filter:auth.init", "method": "getStrategy", "callbacked": false
		},
		{
			"hook": "filter:admin.header.build", "method": "addMenuItem", "callbacked": false
		},
		{
			"hook": "filter:admin.create_routes", "method": "addAdminRoute", "callbacked": true
		}
	],
	"staticDir": "./static"
}